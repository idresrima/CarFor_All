<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddCar">

  <androidx.gridlayout.widget.GridLayout
      android:layout_width="418dp"
      android:layout_height="437dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent">

    <View
        android:id="@+id/join_view"
        android:layout_width="214dp"
        android:layout_height="190dp"
        android:background="#8BB8B8"
        app:layout_column="0"
        app:layout_row="0" />

    <View
        android:id="@+id/view88"
        android:layout_width="199dp"
        android:layout_height="204dp"
        android:background="#9BCACA"
        app:layout_column="1"
        app:layout_row="1" />

    <View
        android:id="@+id/askcar_view"
        android:layout_width="214dp"
        android:layout_height="205dp"
        android:background="#9FC6BD"
        app:layout_column="0"
        app:layout_row="1" />

    <View
        android:id="@+id/addcar_view"
        android:layout_width="201dp"
        android:layout_height="190dp"
        android:background="#9BB8B1"
        app:layout_column="1"
        app:layout_row="0" />

  </androidx.gridlayout.widget.GridLayout>




  </androidx.constraintlayout.widget.ConstraintLayout>